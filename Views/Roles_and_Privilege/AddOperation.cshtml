@{
    ViewBag.Title = "Agregar Operacion";
    string exception = ViewBag.ExceptionMessage;
    string Info = ViewBag.InfoMessage;
}
<div class="jumbotron">
    <h1>Agregar Operacion</h1>
    <p class="lead"></p>
    <p class="lead">
    </p>
    @if (exception != null)
    {
        <div class="alert alert-danger" role="alert">
            @exception
        </div>
    }
    @if (Info != null)
    {
        <div class="alert alert-info" role="alert">
            @Info
        </div>
    }
</div>

