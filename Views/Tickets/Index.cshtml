                    <th colspan="1">Folio</th>
                    <th colspan="2">Titulo</th>
                    <th colspan="2">Ultimo comentario</th>
                    <th colspan="1">Status</th>
                    <th colspan="1">Terminal</th>
                    <th colspan="2">Accion</th>
                </tr>

