@model TAS360.Models.ViewModel.auditoriaViewModel
@{
    ViewBag.Title = "Test PTS";
}

<div class="jumbotron">
    <h1>Auditoria</h1>
    <p class="lead">Tabla donde llegan los registros de los movimientos internos realizados en TAD </p>
</div>
<section class="page-section cta">
    <div class="container">
        <div class="row">
            <div class="col-xl-9 mx-auto">
                <div class="cta-inner bg-faded text-center rounded">
                    <div class="row">
                        <div class="col-lg-12">
                            <table class="table table-bordered">
                                <tr>
                                    <th>Identificador</th>
                                    <th>Fecha</th>
                                    <th>Hora</th>
                                    <th>Estado</th>
                                    <th>Flujo de datos</th>
                                    <th>Secuencial</th>
                                    <th>Transacción (TRAMA)</th>
                                    @*<th>Operación</th>*@
                                </tr>
                                @foreach (var usr in Model.auditoria)
                                {
                                    <tr>
                                        <td>@usr.Id</td>
                                        <td>@usr.Fecha</td>
                                        <td>@usr.Hora</td>
                                        <td>@usr.Estado</td>
                                        <td>@usr.Flujo_datos</td>
                                        <td>@usr.secuencial</td>
                                        <td>@usr.Transaccion</td>
                                        @*<td>
                                            <input type="button" class="btn btn-default" onclick="document.location.href = '@Url.Content("~/home/InterpretarTrama/" + usr.Id)'" value="Interpretar" />
                                            <input type="button" class="btn btn-danger" value="Eliminar" />
                                        </td>*@
                                    </tr>
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>