@model TAS360.Models.ViewModel.salidasViewModel
@{
    ViewBag.Title = "Test PTS";
}

<div class="jumbotron">
    <h1>Salidas</h1>
    <p class="lead">Tabla donde llegan los registros del SIIC - SIMCOT </p>
    @*<p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>*@
</div>
<section class="page-section cta">
    <div class="container">
        <div class="row">
            <div class="col-xl-9 mx-auto">
                <div class="cta-inner bg-faded text-center rounded">
                    <div class="row">
                        <div class="col-lg-12">
                            <table class="table table-bordered">
                                <tr>
                                    <th>Identificador</th>
                                    <th>Transacción (TRAMA)</th>
                                    <th>Operación</th>
                                </tr>
                                @foreach (var usr in Model.salidas)
                                {
                                    <tr>
                                        <td>@usr.Id</td>
                                        <td>@usr.Transaccion</td>
                                        <td>
                                            <input type="button" class="btn btn-default" onclick="document.location.href = '@Url.Content("~/home/InterpretarTrama/" + usr.Id)'" value="Interpretar" />
                                            <input type="button" class="btn btn-danger" value="Eliminar" />
                                        </td>
                                    </tr>
                                }
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@if (Model.Trama != null)
{
<div class="row">
    <h2>Tipo de Transaccion</h2>
    <div class="col-md-4">

        <table class="table table-bordered">
            <tr>
                <th>Dato</th>
                <th>Valor</th>
            </tr>
            <tr>
                <td>Tipo de transaccion</td>
                <td>@Model.Trama.TipoTransaccion</td>
            </tr>
            <tr>
                <td>Numero de Operacion</td>
                <td>@Model.Trama.NumeroOperacion</td>
            </tr>
            <tr>
                <td>Numero de compartimiento</td>
                <td>@Model.Trama.NumeroCopartimiento</td>
            </tr>
            <tr>
                <td>Estado de transaccion</td>
                <td>@Model.Trama.EstadoTransaccion</td>
            </tr>
        </table>
    </div>

    <div class="col-md-4">
        
        <table class="table table-bordered">
            <tr>
                <th>Dato</th>
                <th>Valor</th>
            </tr>
            <tr>
                <td>Reazones de cancelacion</td>
                <td>@Model.Trama.RazonCancelacion</td>
            </tr>
            <tr>
                <td>Modulo de Operacion</td>
                <td>@Model.Trama.ModuloOperqcion</td>
            </tr>
            <tr>
                <td>Vehiculo</td>
                <td>@Model.Trama.identificacionVehiculo</td>
            </tr>
            <tr>
                <td>Producto Anterior</td>
                <td>@Model.Trama.codigoProductoAnterior</td>
            </tr>
        </table>
    </div>

    <div class="col-md-4">
        
        <table class="table table-bordered">
            <tr>
                <th>Dato</th>
                <th>Valor</th>
            </tr>
            <tr>
                <td>Volumen Natural</td>
                <td>@Model.Trama.VolumenNatural</td>
            </tr>
            <tr>
                <td>clave del cliente</td>
                <td>@Model.Trama.ClaveCliente</td>
            </tr>
            <tr>
                <td>Producto Nuevo</td>
                <td>@Model.Trama.CodigoProductoNuevo</td>
            </tr>
            <tr>
                <td>Densidad</td>
                <td></td>
            </tr>
        </table>
    </div>

</div>
}

